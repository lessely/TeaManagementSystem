<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zdx.studentces">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitiy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activitiy.Manager_login" />
        <activity android:name=".Activitiy.Manager_Menu" />
        <activity android:name=".Activitiy.Staff_login" />
        <activity android:name=".Activitiy.Staff_Menu" />
        <activity android:name=".Activitiy.add_staff_info" />
        <activity android:name=".Activitiy.add_tea_info" />
        <activity android:name=".Activitiy.query_staff_info" />
        <activity android:name=".Activitiy.query_tea_info" />
        <activity android:name=".Activitiy.out_tea_info" />
        <activity android:name=".Activitiy.web" />

    </application>

</manifest>